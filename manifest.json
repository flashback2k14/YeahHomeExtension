{
  "name": "Yeah! Home Extension",
  "description": "This extension allows you to go to https://www.google.com.",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "app/images/ic_home_white_48dp_2x.png",
    "default_title": "Yeah! Home"
  },
  "background": {
    "scripts": ["app/background.js"],
    "persistent": false
  },
  "options_page": "app/options.html",
  "options_ui": {
    "page": "app/options.html",
    "chrome_style": false
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}